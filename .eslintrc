 {
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:jsx-a11y/recommended"
  ],
   "plugins": ["react", "import", "jsx-a11y"],
   "parserOptions": {
     "sourceType": "module"
   },
   "rules": {
     "react/prop-types": "off",
     "no-unused-vars": "off"
   }
}